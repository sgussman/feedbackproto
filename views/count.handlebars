<!doctype html>
<html>
<head>
	<title>Class Feedback</title>
	<meta charset="utf-8">
	<meta http-equiv="refresh" content="10">
	<link href="http://netdna.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.css" rel="stylesheet">
</head>

<body>
	<table style="width: 100%">
		<th style="width: 100%">
			<td>Total Count: {{total}}</td>
			<td>Green Clicks: {{green}}</td>
			<td>Yellow Clicks: {{yellow}}</td>
			<td>Red Clicks: {{red}}</td>
		</th>
	<div>
		<h2>Count</h2>
		{{#each clicks}}
		<div class="click">
			<h3> {{action}} {{name}} {{time}} {{comment}}</h3>
		</div>
		{{/each}}

	</div>
	<script src="http://code.jquery.com/jquery-1.9.1.js"></script>

</body>
</html>